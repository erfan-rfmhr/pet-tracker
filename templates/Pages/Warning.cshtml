@page
@model WarningModel
@{
    ViewData["Title"] = "Error";
}
<div id="page-content">
    <h1 class="text-danger">Access denied.</h1>
    <h2 class="text-danger">You do not have access to view this page.</h2>

    @if (Model.ShowRequestId)
    {
        <p>
            <strong>Request ID:</strong> <code>@Model.RequestId</code>
        </p>
    }


